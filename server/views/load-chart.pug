script.
    window.onload = function() {
        var dataPoints = [];
        var frameRateManager =
                !{JSON.stringify(session.metricsManagers.find(x => x.name == 'Frame rate'))};
        frameRateManager.metrics.forEach(function(metric) {
            if(metric.val >= frameRateManager.thresholds.average) {
                var lineColor = "#ffdc32";
                var color = "#ccff00";
            }
            else {
                var lineColor = "#ffdc32";
                var color = "#ff3f00";
            }
            point = {
                x: metric.recordTime,
                y: metric.val,
                lineColor: lineColor,
                color: color
            };
            dataPoints.push(point);
        });
        var content = {
            theme: "dark2",
            title: {
                text: "Frame rate chart"
            },
            data: [
                {
                    type: "line",
                    dataPoints: dataPoints
                }
            ],
            axisY: {
                title: "FPS",
                minimum: 60,
                stripLines: [
                    {
                        value: frameRateManager.thresholds.average,
                        label: "Threshold"
                    }
                ],
            },
            axisX: {
                title: "Seconds"
            }
        };
        var chart = new CanvasJS.Chart('frame-rate-chart-container', content);
        chart.render();
    }
